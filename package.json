    "autoless": "^0.1.6",
    "babel": "^4.7.16",
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "es5-shim": "^4.1.0",
    "http-server": "^0.8.0",
    "jsx-loader": "^0.12.2",
    "less": "^2.4.0",
    "mkdirp": "^0.5.0",
    "mocha": "^2.2.1",
    "mocha-phantomjs": "^3.5.3",
    "parallelshell": "^1.1.1",
    "phantomjs": "^1.9.16",
    "should": "^5.2.0",
    "webpack": "^1.7.3"
  }
